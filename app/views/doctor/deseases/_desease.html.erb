<tr>
  <td><%= desease_counter + 1 %></td>
  <td><%= desease.pet_type %></td>
  <td><%= desease.name %></td>
  <td style="white-space: nowrap; overflow: hidden; text-overflow: ellipsis; max-width: 400px;"><%= desease.symptom %></td>
  <td class="text-right">
      <div class="dropdown dropdown-action">
          <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
          <div class="dropdown-menu dropdown-menu-right">
              <a class="dropdown-item" href="<%= edit_doctor_desease_path(desease) %>"><i class="fa fa-pencil m-r-5"></i> Edit</a>
              <a class="dropdown-item" href="#" data-toggle="modal" data-target="#delete-<%= desease.id %>"><i class="fa fa-trash-o m-r-5"></i> Delete</a>
          </div>
      </div>
  </td>
</tr>
<div id="delete-<%= desease.id %>" class="modal fade delete-modal" role="dialog">
			<div class="modal-dialog modal-dialog-centered">
				<div class="modal-content">
					<div class="modal-body text-center">
						<h3>Bạn có muốn xóa triệu chứng này không?</h3>
						<div class="m-t-20"> <a href="#" class="btn btn-white" data-dismiss="modal">Close</a>
							<%= link_to "Xóa", doctor_desease_path(desease),method: :delete, class: "btn btn-danger" %>
						</div>
					</div>
				</div>
			</div>
		</div>
